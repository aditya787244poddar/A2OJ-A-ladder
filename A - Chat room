#include <cstdio>
#include <iostream>
#include <cstring>

int main()
{
    char str[105];
    scanf("%s",str);
    
    char hel[6]="hello";
    
    int i=0,j=0;
    while(str[i]&&hel[j])
    {
    	
        if(str[i]==hel[j])
        {
            i++;
            j++;
            
        }
        else
        i++;
    }
    
    if(j==5)
    printf("YES");
    else
    printf("NO");
}
