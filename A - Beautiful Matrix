#include <cstdio>
#include <cstdlib>
#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int arr[6][6];
    int indexi,indexj;
    for(int i=1;i<6;i++)
    {
        for(int j=1;j<6;j++)
        {
         scanf("%d",&arr[i][j]);
         if(arr[i][j]==1)
         {
             indexi=i;
             indexj=j;
         }
        }
    }
    printf("%d",(abs(indexi-3)+abs(indexj-3)));
}
