#include <cstdio>
#include <iostream>

using namespace std;

int main()
{
    int n,k;
    scanf("%d%d",&n,&k);
    int a[n];
    int count=0;
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]<=(5-k))
        count++;
    }
    count= count/3;
    printf("%d",count);
    
    
    
    
}
